<nav class="cyan" role="navigation">
  <div class="nav-wrapper container">
    <span class="brand-logo hide-on-large-only"><a href="<%= path %>/"><%= title %></a></span>
    <ul class="hide-on-med-and-down">
      <li class="brand-logo" style="position: initial;"><h1><a href="<%= path %>/"><%= title %></a></h1></li>
      <% if (session && (session.TA || session.owner)) { %>
      <li style="margin-left: 10px;"><form method="POST" action="<%= path %>/options">
      <% if (typeof frozen != 'undefined' && frozen) { %>
      <button class="waves-effect btn blue lighten-5 black-text freeze-btn freeze-input" name="frozen" value="0">Unfreeze</button>
      <% } else if (typeof frozen != 'undefined') { %>
      <button class="waves-effect btn blue lighten-5 black-text freeze-btn freeze-input" name="frozen" value="1">Freeze</button>
      <% } %>
      </form></li>
      <% } %>
    </ul>
    <ul class="right hide-on-med-and-down">
      <% if (session && session.authenticated) { %>
      <li><span class="blue-text text-lighten-4">Logged in as <%=session.user_id %></span>
      <% if (session.TA || session.owner) { %>
      <a class="waves-effect" href="<%= path %>/settings">Settings</a>
      <a class="waves-effect" href="<%= path %>/metrics">Metrics</a>
      <% } %>
      <a class="waves-effect" href="<%= path %>/logout">Log Out</a></li>
      <% } else { %>
      <li><a class="waves-effect" href="<%= path %>/login">Log In</a></li>
      <% } %>
    </ul>

    <ul id="nav-mobile" class="sidenav">
      <% if (session && session.authenticated) { %>
      <li class="cyan white-text" style="padding: 0px 20px;">Logged in as <%= session.user_id %></li>
      <% if (session.TA || session.owner) { %>
      <li><form method="POST" action="<%= path %>/options">
      <% if (typeof frozen != 'undefined' && frozen) { %>
      <input type="hidden" name="frozen" class="freeze-input" value="0">
      <a onclick="$(this).closest('form').submit();"><span class="freeze-btn">Unfreeze</span> Queue</a>
      <% } else if (typeof frozen != 'undefined') { %>
      <input type="hidden" name="frozen" class="freeze-input" value="1">
      <a onclick="$(this).closest('form').submit();"><span class="freeze-btn">Freeze</span> Queue</a>
      <% } %>
      </form></li>
      <% } %>
      <% if (session.TA || session.owner) { %>
      <li><a href="<%= path %>/settings">Settings</a></li>
      <li><a href="<%= path %>/metrics">Metrics</a></li>
      <% } %>
      <li><a href="<%= path %>/logout">Log Out</a></li>
      <% } else { %>
      <li><a class="waves-effect" href="<%= path %>/login">Log In</a></li>
      <% } %>
    </ul>
    <a href="#" data-target="nav-mobile" class="sidenav-trigger"><i class="material-icons">menu</i></a>
  </div>
</nav>
<script type="text/javascript">
$(document).ready(function() {
    $(".sidenav").sidenav();
});
</script>
